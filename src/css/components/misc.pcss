}

.single-accordion {
  --contentRows: 0fr;
  --sectionPadTopFactor: 0;
  --buttonTextColor: var(--colorWhite);
  &.-open {
    --contentRows: 1fr;
    --buttonTextColor: var(--colorBlack);
  }
  .-head {
    @mixin center-frame;
    display: flex;
    justify-content: center;
    padding-bottom: 50px;
    button {
      background-color: var(--brandTeal);
      padding: 30px 80px 36px;
      border-radius: 14px;
      color: var(--buttonTextColor);
    }
  }
  .-expander {
    display: grid;
    grid-template-rows: var(--contentRows);
    transition: grid-template-rows 0.5s var(--transitionSlowFastSlow);
    .-height {
      overflow: hidden;
      /* .-inner {
        padding: 60px 0;
      } */
    }
  }
  @media (max-width: 1023px) {
    .-head {
      button {
        width: 100%;
      }
    }
  }
}

