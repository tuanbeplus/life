

.video-grid {
  display: grid;
  grid-template-columns: 1fr 1fr;
  max-width: 1220px;
  gap: 20px;
  .vid-player {
    width: 100%;
  }
  @media (max-width: 800px) {
    grid-template-columns: 1fr;
    max-width: 500px;
  }
}

.local-overlay {
  @mixin inset-fixed;
  pointer-events: none;
  z-index: 100;
  &.-show {
    pointer-events: auto;
  }
}

@keyframes parallax {
  to {
    transform: translateY(1000px);
  }
}
@keyframes parallax-half {
  to {
    transform: translateY(600px);
  }
}


